	<!-- Site Loader -->
	<div id="site-loader" aria-hidden="true" role="presentation">
		<img src="/pictures/logo/Washology.png" alt="Washology" />
	</div>
	<script>
		window.addEventListener('load', function () {
			var el = document.getElementById('site-loader');
			if (!el) return;
			el.classList.add('hidden');
			setTimeout(function(){ try { el.remove(); } catch(e) { if (el && el.parentNode) el.parentNode.removeChild(el); } }, 400);
		});
	</script>
	<!-- Force header Contact links to navigate (do not open ZIP modal) -->
	<script>
	(function(){
	  function sanitizeLink(a){
	    try {
	      a.removeAttribute('data-bs-toggle');
	      a.removeAttribute('data-bs-target');
	      // Stop any other scripts from intercepting the click; allow default navigation to proceed
	      a.addEventListener('click', function(e){ e.stopImmediatePropagation(); }, true);
	    } catch(_) {}
	  }
	  document.addEventListener('DOMContentLoaded', function(){
	    // Header nav and banner contact links
	    document.querySelectorAll('header a[href="/contact-us"]').forEach(sanitizeLink);
	    var banner = document.querySelector('.banner-header a');
	    if (banner && /\/contact-us$/.test(banner.getAttribute('href')||'')) sanitizeLink(banner);
	  });
	})();
	</script>
